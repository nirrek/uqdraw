{
  "name": "uqdraw",
  "version": "1.0.0",
  "description": "Platform for asking in-lecture questions and getting free-form responses.",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "testi": "mocha --compilers js:babel-core/register.js ./js/__integrationTests__/*.js",
    "start": "node devServer.js",
    "build:webpack": "NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "build:copy": "cp favicon.ico index.html build",
    "build": "npm run build:webpack && npm run build:copy",
    "deploy": "surge build uqdraw.co"
  },
  "author": "nirrek",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel": "^6.5.2",
    "babel-eslint": "^5.0.0-beta4",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "expect.js": "^0.3.1",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "firebase-server": "^0.2.0",
    "image-webpack-loader": "^1.6.3",
    "imagemin": "^4.0.0",
    "jest": "^0.1.40",
    "jest-cli": "^0.8.2",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.1",
    "react-hot-loader": "^1.2.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "^2.7.1"
  },
  "dependencies": {
    "firebase": "^2.4.1",
    "flux": "^2.0.3",
    "keymirror": "^0.1.1",
    "normalize.css": "^3.0.2",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-modal": "0.6.1",
    "react-router": "^2.0.0",
    "react-spinkit": "^1.0.2",
    "react-style": "^0.5.1"
  },
  "jest": {
    "scriptPreprocessor": "jestPreprocessor.js",
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "react",
      "keymirror",
      "babel-runtime"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "testFileExtensions": [
      "js"
    ],
    "testPathDirs": [
      "js"
    ]
  }
}
